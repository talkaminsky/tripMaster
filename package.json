{
  "name": "trip-master",
  "version": "1.0.0",
  "description": "Trip Master",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/main.scss src/assets/css/main.css",
    "minify-sass": "node-sass src/assets/scss/amain.scss src/assets/css/main.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/main.scss src/assets/css/main.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:5000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "2.7.3",
    "classnames": "2.2.6",
    "google-maps-react": "^2.0.2",
    "is-empty": "^1.2.0",
    "jwt-decode": "^2.2.0",
    "moment": "2.24.0",
    "node-sass": "7.0.0",
    "nouislider": "13.1.1",
    "npm": "^6.13.6",
    "react": "16.8.4",
    "react-chartjs-2": "2.7.4",
    "react-copy-to-clipboard": "5.0.1",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.4",
    "react-load-script": "^0.0.6",
    "react-redux": "^7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "2.1.8",
    "reactstrap": "7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/googlemaps": "3.30.18",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.8.7",
    "typescript": "3.3.3333"
  }
}
